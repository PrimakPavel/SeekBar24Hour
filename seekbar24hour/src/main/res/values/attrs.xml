<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LineGraphView">
        <attr name="lgv_backgroundColor" format="color"/>
        <attr name="lgv_mainEventColor" format="color"/>
        <attr name="lgv_markEventColor" format="color"/>
        <attr name="lgv_textColor" format="color"/>
        <attr name="lgv_textSize" format="dimension"/>
        <attr name="lgv_borderHeight" format="dimension"/>
        <attr name="lgv_state" format="enum">
            <enum name="enable" value="0" />
            <enum name="disable" value="1" />
        </attr>
        <attr name="lgv_minDivider" format="enum">
            <enum name="min" value="0" />
            <enum name="sec" value="1" />
        </attr>
    </declare-styleable>


    <declare-styleable name="SeekBar24HourView">
        <attr name="sb24h_backgroundColor" format="color"/>
        <attr name="sb24h_mainEventColor" format="color"/>
        <attr name="sb24h_markEventColor" format="color"/>
        <attr name="sb24h_textColor" format="color"/>
        <attr name="sb24h_textSize" format="dimension"/>
        <attr name="sb24h_textFontFamilyName" format="string"/>
        <attr name="sb24h_textFontStyle" format="integer"/>
        <attr name="sb24h_borderHeight" format="dimension"/>
        <attr name="sb24h_divLineHeight" format="dimension"/>
        <attr name="sb24h_divLineWidth" format="dimension"/>
        <attr name="sb24h_state" format="enum">
            <enum name="enable" value="0" />
            <enum name="disable" value="1" />
        </attr>
        <attr name="sb24h_minDivider" format="enum">
            <enum name="min" value="0" />
            <enum name="sec" value="1" />
        </attr>
    </declare-styleable>

</resources>