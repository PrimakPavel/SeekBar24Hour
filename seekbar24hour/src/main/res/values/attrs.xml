<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomView">
        <attr name="faceColor" format="color"/>
        <attr name="eyesColor" format="color"/>
        <attr name="mouthColor" format="color"/>
        <attr name="borderColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="state" format="enum">
            <enum name="happy" value="0" />
            <enum name="sad" value="1" />
        </attr>

    </declare-styleable>

    <declare-styleable name="LineGraphView">
        <attr name="LgvBackgroundColor" format="color"/>
        <attr name="LgvMainEventColor" format="color"/>
        <attr name="LgvMarkEventColor" format="color"/>
        <attr name="LgvTextColor" format="color"/>
        <attr name="LgvTextSize" format="dimension"/>
        <attr name="LgvBorderHeight" format="dimension"/>
        <attr name="LgvState" format="enum">
            <enum name="enable" value="0" />
            <enum name="disable" value="1" />
        </attr>
        <attr name="LgvMinDivider" format="enum">
            <enum name="min" value="0" />
            <enum name="sec" value="1" />
        </attr>
    </declare-styleable>


    <declare-styleable name="SeekBar24HourView">
        <attr name="Sb24hBackgroundColor" format="color"/>
        <attr name="Sb24hMainEventColor" format="color"/>
        <attr name="Sb24hMarkEventColor" format="color"/>
        <attr name="Sb24hTextColor" format="color"/>
        <attr name="Sb24hTextSize" format="dimension"/>
        <attr name="Sb24hBorderHeight" format="dimension"/>
        <attr name="Sb24hDivLineHeight" format="dimension"/>
        <attr name="Sb24hDivLineWidth" format="dimension"/>
        <attr name="Sb24hState" format="enum">
            <enum name="enable" value="0" />
            <enum name="disable" value="1" />
        </attr>
        <attr name="Sb24hMinDivider" format="enum">
            <enum name="min" value="0" />
            <enum name="sec" value="1" />
        </attr>
    </declare-styleable>

</resources>